class TestClass
{
    init()
    {
        self.item = "initializer"
        self.prop = 0
    }

    print_object()
    {
        print(self.item)
        self.prop += 1
    }

    get_item()
    {
        return self.item
    }
}

var test = TestClass()
test.print_object()
assert(test.get_item() == "initializer")
assert(test.prop == 1)
print('class with field test passed!')