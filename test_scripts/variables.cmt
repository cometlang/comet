import 'unittest' as unittest

function test_variables() {
    var number = 3

    number += 2
    unittest.Assert.that(number).is_equal_to(5)

    number -= 1
    unittest.Assert.that(number).is_equal_to(4)

    number /= 2
    unittest.Assert.that(number).is_equal_to(2)

    number *= 3
    unittest.Assert.that(number).is_equal_to(6)

    number %= 4
    unittest.Assert.that(number).is_equal_to(2)
}
